{
  "name": "webpack4-starter",
  "version": "1.0.0",
  "description": "template webpack4+prettier+eslint+tailwindcss",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint ./src --max-warnings=0",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix",
    "eslint-check": "./node_modules/.bin/eslint --print-config . | ./node_modules/.bin/eslint-config-prettier-check",
    "dev": "webpack --mode development --config webpack.dev.js",
    "build": "webpack --mode production --config webpack.prod.js",
    "start": "webpack-dev-server --mode development --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unasol/webpack4-starter.git"
  },
  "keywords": [
    "webpack4-template"
  ],
  "author": "Una Solutions SAC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unasol/webpack4-starter/issues"
  },
  "homepage": "https://github.com/unasol/webpack4-starter#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@glidejs/glide": "^3.3.0",
    "app-manifest-webpack-plugin": "^1.2.0",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "1.18.2",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tailwindcss": "^1.0.4",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "> 1%",
